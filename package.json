                "node-addons"
            ]
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "roots": [
            "<rootDir>/tests/",
            "<rootDir>/app"
        ],
        "moduleDirectories": [
            "resources/assets/js",
            "resources/assets/store",
            "app/FaveoLog/views/js",
            "app/Bill/views/js",
            "node_modules",
            "app/Notify/views/js"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/@vue/vue3-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(epic-spinners)/)",
            "vendor/*"
        ],
        "setupFiles": [
            "jest-localstorage-mock",
            "<rootDir>/jest.setup.js"
        ],
        "setupFilesAfterEnv": ["<rootDir>/setupAfterEnv.js"],
        "moduleNameMapper": {
            "eonasdan-bootstrap-datetimepicker/(.*)\\.(css)$": "<rootDir>/node_modules/eonasdan-bootstrap-datetimepicker",
            "intl-tel-input": "<rootDir>/__mocks__/intl-tel-input.js",
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        }
